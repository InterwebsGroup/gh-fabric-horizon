    "section_GFiYp7": {
      "type": "section",
      "blocks": {
        "text_3P4jte": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>Frequently asked questions</h3>",
            "width": "fit-content",
